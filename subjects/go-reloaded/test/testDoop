echo "-=-=-===========-=-=-"
echo "-=-=- Test Doop -=-=-"
echo "-=-=-===========-=-=-"
cd ../doop
echo "===== Main Tests ====="
go run . 861 + 870 | cat -e
echo "1731"
echo "========="
go run . 861 - 870 | cat -e
echo "-9"
echo "========="
go run . 861 "*" 870 | cat -e
echo "749070"
echo "========="
go run . 861 % 870 | cat -e
echo "861"
echo "========="
go run . 861 % 870 | cat -e
echo "861"
echo "========="
go run . hello + 1 | cat -e
echo "0"
echo "========="
go run . 1 p 1 | cat -e
echo "0"
echo "========="
go run . 1 / 0 | cat -e
echo "No division by 0"
echo "========="
go run . 1 % 0 | cat -e
echo "No modulo by 0"
echo "========="
go run . 1 "*" 1 | cat -e
echo "1"
echo "========="
go run . 9223372036854775807 + 1
echo "0"
echo "========="
go run . -9223372036854775809 - 3
echo "0"
echo "========="
go run . 9223372036854775807 "*" 3
echo "0"
echo "========="

echo "=== Bonus Tests ==="
go run . 1 + 1 | cat -e
echo "2"
echo "========="
go run . 1 "*" 1
echo "1"
echo "========="
go run . 1 "*" -1
echo "-1"
echo "========="
go run . -9223372036854775808 "*" -1
echo "0"
echo "========="
go run . 9223372036854775807 "-" -1
echo "0"
echo "========="
go run . -9223372036854775808 "-" 1
echo "0"
echo "========="
go run . -9223372036854775805 "-" 100
echo "0"
echo "========="
go run . -9223372036854775808 "+" -1
echo "0"
echo "========="
go run . -9223372036854775809 "+" 100
echo "0"
echo "========="
go run . -9223372036854775808 "/" -1
echo "0"
echo "========="
go run . -9223372036854775808 "*" 9223372036854775807
echo "0"
echo "========="
go run . -9223372036854 "*" 9223372036854
echo "0"
echo "========="
go run . -9223372036854775808 "*" 3
echo "0"
echo "========="
go run . -9223372036854775808 "*" 1
echo "-9223372036854775808"
echo "========="
go run . -9223372036854775807 "*" -1
echo "9223372036854775807"
echo "========="
go run . 1 "*" -9223372036854775808
echo "-9223372036854775808"
echo "========="
go run . -1 "*" -9223372036854775807
echo "9223372036854775807"
echo "========="